//过滤无效播放链接，填域名或者中间段关键词
"过滤词":"",
"播放请求头": "",

/**
搜索可包含页码，会把搜索到的全部内容展示出来
模式1：截取模式，json模式很多网站不支持
模式0：json模式，自动截取内容 
模式0：详情无数据时，可在搜索后缀中填入详情链接，如："搜索后缀":"/detail/",
post格式的搜索是模式1，类似这样的写法：搜索链接;post;参数1;参数2。多个参数也是用;分隔。例如："搜索url": "https://v.xiangdao.me/index.php?m=vod-search;post;wd={wd}",
*/
"搜索url": "https://dm84.tv/index.php/ajax/suggest?mid=1&wd={wd}&limit=20",
"搜索模式": "0",
"搜索二次截取": "",
"搜索数组": "",
"搜索图片": "pic",
"搜索标题": "name",
"搜索链接": "id",
"搜索链接前缀": "https://dm84.tv/v/",
"搜索链接后缀": ".html",
"搜索后缀": "空",

/**
